# Project Settings
PROJECT_NAME="Dollar Mining"
API_V1_STR="/api/v1"
SECRET_KEY="your_secret_key_here"

# Database (SQLite)
DATABASE_URL="sqlite:///./trading.db"

# Korea Investment API (KIS)
# Format: JSON string of list of accounts
# Each account must have: name, broker, account_no, app_key, app_secret
# broker: "KIS" (해외) or "KIS_DOMESTIC" (국내)
ACCOUNTS='[{"name": "한투해외", "broker": "KIS", "account_no": "XXXXXXXX-01", "app_key": "your_app_key", "app_secret": "your_app_secret"}]'
KIS_BASE_URL="https://openapi.koreainvestment.com:9443"
# KIS_BASE_URL="https://openapivts.koreainvestment.com:29443" # Paper Trading

# Discord Webhook (for notifications - Daily Summary at 7 AM)
# Format: JSON with channel names as keys
DISCORD_WEBHOOK_URL='{"private": "https://discord.com/api/webhooks/...", "public": "https://discord.com/api/webhooks/..."}'

# Discord Bot (for interactive commands - price, balance, holdings)
DISCORD_BOT_TOKEN="your_discord_bot_token"
DISCORD_CHANNEL_ID='{"private": "channel_id", "public": "channel_id"}'
DISCORD_BOT_DEFAULT_ACCOUNT="한투해외"

# Gemini API (for AI chat features - free tier available)
# Get your key at: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=""

# Timezone
TZ="Asia/Seoul"
